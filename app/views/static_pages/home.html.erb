<% provide(:title, 'Home') %>
<% if signed_in? %>
	<div class="row">
		<aside class="span4">
			<section><%= render 'shared/user_info' %></section>
      <section><%= render 'shared/stats' %></section>
			<section><%= render 'shared/micropost_form' %></section>
		</aside>
		<div class="span8">
			<h3>Micropost Feed</h3>
			<%= render 'shared/feed' %>
		</div>
	</div>
<% else %>
	<%= render 'default' %>
<% end %>

 <button class="win-command" rel="tootlip" title="Simple command with only an icon">
<span class="win-commandicon icon-cog-4"></span>
</button>